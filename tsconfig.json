{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    ".astro/types.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.astro",
    "src/env.d.ts",
    "astro.config.mjs",
    "eslint.config.mjs",
    "eslint.config.mjs"
  ],
  "exclude": ["node_modules", "dist"]
}
